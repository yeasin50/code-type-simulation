{
  "name": "code-type-simulation",
  "displayName": "code_type_simulator",
  "description": "type-code-on-specific-index",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.94.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
